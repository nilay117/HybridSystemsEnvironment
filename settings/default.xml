<edu.ucsc.cross.hybrid.env.core.containers.SettingConfiguration>
  <settings>
    <entry>
      <java-class>edu.ucsc.cross.hybrid.env.core.settings.DataSettings</java-class>
      <edu.ucsc.cross.hybrid.env.core.settings.DataSettings>
        <automaticallyStoreResults>true</automaticallyStoreResults>
        <storeDataAtIncrements>true</storeDataAtIncrements>
        <storeAtEveryJump>false</storeAtEveryJump>
        <dataStoreIncrement>0.1</dataStoreIncrement>
        <autoStoreDirectory>./results</autoStoreDirectory>
        <environmentNameSubDirectory>true</environmentNameSubDirectory>
        <storePreJumpValue>false</storePreJumpValue>
      </edu.ucsc.cross.hybrid.env.core.settings.DataSettings>
    </entry>
    <entry>
      <java-class>edu.ucsc.cross.hybrid.env.core.settings.PrintSettings</java-class>
      <edu.ucsc.cross.hybrid.env.core.settings.PrintSettings>
        <printProgressUpdates>true</printProgressUpdates>
        <totalSimTimePrintOuts>20</totalSimTimePrintOuts>
        <printDiscreteEventIndicator>true</printDiscreteEventIndicator>
        <printStoreDataIndicator>true</printStoreDataIndicator>
        <printStoreDataReport>true</printStoreDataReport>
      </edu.ucsc.cross.hybrid.env.core.settings.PrintSettings>
    </entry>
    <entry>
      <java-class>edu.ucsc.cross.hybrid.env.core.settings.ComputationSettings</java-class>
      <edu.ucsc.cross.hybrid.env.core.settings.ComputationSettings>
        <odeMinStep>0.01</odeMinStep>
        <odeMaxStep>0.1</odeMaxStep>
        <odeScalAbsoluteTolerance>1.0E-6</odeScalAbsoluteTolerance>
        <odeScalRelativeTolerance>1.0E-6</odeScalRelativeTolerance>
        <ehMaxCheckInterval>0.001</ehMaxCheckInterval>
        <ehConvergence>0.001</ehConvergence>
        <ehMaxIterationCount>10</ehMaxIterationCount>
        <jumpPriority>true</jumpPriority>
        <maxRecursiveStackSize>1000</maxRecursiveStackSize>
        <integrator>DORMAND_PRINCE_853</integrator>
        <integratorOptions>
         Integrator Options :
         - Euler = EULER
         - Dormand Prince 853 = DORMAND_PRINCE_853
         - Dormand Prince 54 = DORMAND_PRINCE_54</integratorOptions>
      </edu.ucsc.cross.hybrid.env.core.settings.ComputationSettings>
    </entry>
    <entry>
      <java-class>edu.ucsc.cross.hybrid.env.core.settings.ExecutionSettings</java-class>
      <edu.ucsc.cross.hybrid.env.core.settings.ExecutionSettings>
        <runThreadded>true</runThreadded>
        <numberOfTrials>1</numberOfTrials>
        <maxParallelExecutions>1</maxParallelExecutions>
        <simDuration>10.0</simDuration>
        <jumpLimit>20</jumpLimit>
      </edu.ucsc.cross.hybrid.env.core.settings.ExecutionSettings>
    </entry>
  </settings>
</edu.ucsc.cross.hybrid.env.core.containers.SettingConfiguration>